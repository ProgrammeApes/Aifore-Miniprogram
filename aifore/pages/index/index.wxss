.container{
  padding: 390rpx 10rpx 112rpx 10rpx;
}
.container.fit-iphonex{
  padding-bottom: 190rpx;
}
.swiper{
  position: absolute;
  top: 6rpx;
  left: 0;
  padding: 0 10rpx;
  width: 100%;
  height: 375rpx;
  margin-bottom: 14rpx;
}
.swiper image{
  width: 100%;
  height: 375rpx;
}
.tab-container{
  max-height: 700rpx;
}
.tab-container>view{
  height: 100%;
}
.tab-container .right-box{
  display: flex;
  flex-direction:column;
  justify-content: flex-start;
  align-items: center;
}
.tab-container .tab{
  display: flex;
  flex-direction:column;
  justify-content: center;
  align-items: center;
  background: #9ccc65;
}
.tab-container .top-box{
  height: 236rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 10rpx;
}
.tab-container .down-box{
  align-items: flex-start;
  flex-wrap: wrap;
  justify-content: flex-start;
  position: relative;
}
.vip{
  position: absolute;
  height: 200%;
  right: 0;
}
.tab.deep-green{
  background: #7cb342;
}
.tab.easy-green{
  background: #c5e1a5;
}
.tab.small-height{
  width: 236rpx;
  height: 236rpx;
}
.tab .tab-text{
  font-size: 28rpx;
  color: #000;
  margin-top: 36rpx;
}