.container{
  position: relative;
  background: #9ccc65;
  padding-bottom: 120rpx;
}
.container.fit-iphonex{
  padding-bottom: 190rpx;
}
.bg{
  position: absolute;
  width: 100%;
}
.btn-start{
  position: absolute;
  z-index: 1;
  left: 17%;
  width: 66%;
  top: 14%;
}
.fit-iphonex>.bottom-img{
  bottom: 120rpx;
}
.container.fit-iphonex .bottom-img{
  bottom: 180rpx;
}
.bottom-img{
  position: absolute;
  width: 100%;
  bottom: 118rpx;
  left: 0;
}
.btn-update{
  position: absolute;
  background: #9ccc65;
  border-radius: 10rpx;
  width: 234rpx;
  height: 70rpx;
  line-height: 70rpx;
  color: #fff;
  font-size: 27rpx;
  text-align: center;
  bottom: 75rpx;
  left: 50%;
  margin-left: -117rpx;
}
.tips{
  position: absolute;
  width: 100%;
  font-size: 24rpx;
  color: #908F8F;
  text-align: center;
  bottom: 10rpx;
}
.poup-box{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: 4;
  background: rgba(0,0,0, .7);
  top: 0;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}
.poup-wrapper{
  position: relative;
  width: 587rpx;
  height: 326rpx;
  background: #fff;
  border-radius: 10rpx;
  padding-top: 54rpx;
  padding-bottom: 120rpx;
}
.poup-wrapper>.title{
  text-align: center;
  font-size: 36rpx;
  color: #000;
  font-weight: bold;
  margin-bottom: 40rpx;
}
.poup-wrapper>.content{
  font-size: 24rpx;
  color: #727372;
  text-align: center;
}
.poup-wrapper>.bottom{
  position: absolute;
  width: 100%;
  bottom: 0;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-top: 2rpx solid #efefef;
}
.poup-wrapper>.bottom>view{
  position: relative;
  flex: 1;
  text-align: center;
  color: #2b2b2b;
  height: 100%;
  line-height: 120rpx;
}
.poup-wrapper>.bottom>view.green{
  color: #7cb342;
  border-left: 2rpx solid #efefef;
}
