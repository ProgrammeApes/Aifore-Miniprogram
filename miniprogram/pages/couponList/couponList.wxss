.container{
  padding: 0 30rpx 100rpx 30rpx;
  background: #fafbf9;
}
.coupon-box{
  position: relative;
  width: 90%;
  height: 372rpx;
  border-radius: 14rpx;
  border-left: 0;
  padding-right: 160rpx;
  padding-left: 80rpx;
  padding-top: 48rpx;
  margin: 0 auto 10rpx auto;
  background: #fff;
}
.coupon-box.first{
  margin-top: 20rpx;
}
.coupon-box .left-bar{
  position: absolute;
  left: 0;
  top: 0;
  width: 32rpx;
  height: 100%;
  background: #9ccc65;
  overflow: hidden;
  border-radius: 14rpx 0 0 14rpx
}
.coupon-box .dot{
  position: absolute;
  width: 26rpx;
  height: 26rpx;
  background: #fff;
  border-radius: 50%;
  top: 50%;
  margin-top: -13rpx;
  left: -13rpx;
}
.coupon-box .coupon-text{
  height: 209rpx;
}
.coupon-box .tag{
  position: absolute;
  width: 66rpx;
  right: 25rpx;
  top: -12rpx;
}
.coupon-box .tag>image{
  position: relative;
  width: 66rpx;
}
.coupon-box .tag .text{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  color: #fff;
  font-size: 17rpx;
  text-align: center;
}
.coupon-box .tag .point{
  font-size: 44rpx;
}
.coupon-box .coupon-img{
  position: absolute;
  width: 143rpx;
  right: 16rpx;
  bottom: 20rpx;
}
.coupon-box .coupon-title{
  font-size: 40rpx;
  margin-bottom: 35rpx;
}
.coupon-box .price{
  font-size: 40rpx;
  color: #DD461D;
  font-weight: bold;
  margin-right: 13rpx;
}
.coupon-box .btn-box{
  justify-content: center;
  font-size: 30rpx;
  color: #85b851;
}
.coupon-box .btn-box>.first{
  border-right: 4rpx solid #a9a9a9;
  padding-left: 0;
}
.coupon-box .btn-box>navigator{
  padding: 0 35rpx;
  height: 67rpx;
  line-height: 67rpx;
  display: block;
}
.line-box{
  margin-bottom: 26rpx;
}
.line-box .dot{
  position: relative;
  width: 14rpx;
  height: 14rpx;
  background: #7cb342;
  border-radius: 50%;
  margin: 0 auto 10rpx auto;
}
.line-box .line{
  border-bottom: 4rpx dashed #cac5bf;
}
.bottom-box{
  position: absolute;
  bottom: 0;
  left: 0;
  background: #fff;
  width: 100%;
  height: 100rpx;
  padding: 0 30rpx;
  border-radius: 10rpx 10rpx 0 0;
  border-top: 1rpx solid #7d7d7d;
}
.bottom-box .point{
  font-size: 24rpx;
  color: #282828;
}
.bottom-box .point>text{
  color: #7cb342;
  font-weight: bold;
  font-size: 50rpx;
  margin-right: 17rpx;
}
.bottom-box .avatar{
  width: 50rpx;
  height: 50rpx;
  margin-right: 16rpx;
}
.bottom-box .text{
  font-size: 24rpx;
}
.bottom-box .icon{
  width: 13rpx;
}